
xdim := 100
ydim := 25

SetGridsize(xdim, ydim, 1)
SetCellsize(500e-9/xdim, 125e-9/ydim, 3e-9)

Msat  = 800e3
Aex   = 13e-12
alpha = 0.02

m = uniform(1, .1, 0)
relax()
saveas(m, "mumax3_s-state")    // relaxed state

autosave(m, 200e-12)
tableautosave(10e-12)

B_ext = vector(-24.6E-3, 4.3E-3, 0)
run(1e-9)
saveas(m, "mumax3_sp4_res")    // relaxed state